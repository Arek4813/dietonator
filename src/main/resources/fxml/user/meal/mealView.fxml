<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="borderPane" prefHeight="431.0" prefWidth="864.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.user.meal.MealViewController">
   <center>
      <VBox prefHeight="431.0" prefWidth="841.0" BorderPane.alignment="CENTER">
         <children>
            <TableView fx:id="mealTableView" prefHeight="351.0" prefWidth="961.0">
               <columns>
                  <TableColumn fx:id="nameTableColumn" prefWidth="117.0" text="Name" />
                  <TableColumn fx:id="energyTableColumn" prefWidth="90.0" text="Energy" />
                  <TableColumn fx:id="fatTableColumn" prefWidth="74.0" text="Fat" />
                  <TableColumn fx:id="saturatesTableColumn" prefWidth="87.0" text="Saturates" />
                  <TableColumn fx:id="carbohydratesTableColumn" prefWidth="129.0" text="Carbohydrates" />
                  <TableColumn fx:id="sugarsTableColumn" prefWidth="85.0" text="Sugars" />
                  <TableColumn fx:id="proteinTableColumn" prefWidth="90.0" text="Protein" />
                  <TableColumn fx:id="saltTableColumn" prefWidth="71.0" text="Salt" />
                  <TableColumn fx:id="ingredientsTableColumn" prefWidth="123.0" text="Ingredients" />
               </columns>
            </TableView>
            <HBox prefHeight="55.0" prefWidth="975.0">
               <children>
                  <TextField fx:id="addMealTextField" />
                  <Button mnemonicParsing="false" onAction="#addMeal" text="Add" />
               </children>
            </HBox>
         </children>
      </VBox>
   </center>
</BorderPane>
